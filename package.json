    "glob": "^5.0.3",
    "jsonfile": "^2.0.0",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.0",
    "moment": "^2.9.0",
    "mustache": "^2.0.0",
    "node-uuid": "^1.4.3",
    "nodegit": "^0.3.3",
    "randomstring": "^1.0.3",
    "request": "^2.54.0",
    "ring": "^2.0.1",
    "sleep": "^2.0.0",
    "slug": "^0.8.0",
    "socket.io": "^1.3.5",
    "sqlite3": "^3.0.5",
    "string": "^3.1.1",
    "tosource": "^0.1.3",
    "underscore": "^1.8.2",
    "validator": "^3.36.0",
    "worker-farm": "^1.2.0",
    "xml2js": "^0.4.6",
    "yamljs": "^0.2.1"
  },
  "devDependencies": {
    "mocha": "^2.2.1",
    "sinon": "^1.14.1",
    "supertest": "^0.15.0"
  },
  "keywords": [
    "continuous integration",
    "testing",
    "ci",
    "cd",
    "ci runner",
    "runner"
  ],
  "config": {},
  "main": "run",
  "scripts": {
    "test": "NODE_PATH=$NODE_PATH:./lib mocha --recursive test",
    "run": "NODE_PATH=$NODE_PATH:./lib node run.js | ./node_modules/.bin/bunyan",
    "pm2_start": "NODE_PATH=$NODE_PATH:./lib pm2 start run.js",
    "pm2_stop": "NODE_PATH=$NODE_PATH:./lib pm2 stop run.js",
    "pm2_delete": "NODE_PATH=$NODE_PATH:./lib pm2 delete run.js",
    "pm2_restart": "NODE_PATH=$NODE_PATH:./lib pm2 restart run.js"
  },
  "engines": {
    "node": ">= 0.10.36"
  }
}
