{
  "name": "sailfish-ci-runner",
  "description": "Simple CI Application - Runner",
  "version": "0.0.2",
  "author": "Albert Lacarta <urodoz@gmail.com>",
  "dependencies": {
    "async": "^0.9.0",
    "docker-command-builder": "0.0.4",
    "exectimer": "^1.0.1",
    "fs-extra": "^0.17.0",
    "glob": "^5.0.3",
    "jsonfile": "^2.0.0",
    "lodash": "^3.7.0",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.0",
    "moment": "^2.9.0",
    "mustache": "^2.0.0",
    "node-uuid": "^1.4.3",
    "serve-static": "^1.9.2",
    "slug": "^0.8.0",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "string": "^3.1.1",
    "validate.js": "^0.7.0",
    "winston": "^1.0.0",
    "worker-farm": "^1.2.0",
    "xml2js": "^0.4.6",
    "yamljs": "^0.2.1"
  },
  "devDependencies": {
    "mocha": "^2.2.1",
    "pre-commit": "^1.0.6",
    "sinon": "^1.14.1"
  },
  "keywords": [
    "continuous integration",
    "testing",
    "ci",
    "cd",
    "ci runner",
    "runner"
  ],
  "config": {},
  "main": "run",
  "scripts": {
    "jshint": "jshint ./lib",
    "test": "NODE_PATH=$NODE_PATH:./lib mocha --recursive test",
    "run": "NODE_PATH=$NODE_PATH:./lib node run.js",
    "pm2_start": "NODE_PATH=$NODE_PATH:./lib pm2 start run.js",
    "pm2_stop": "NODE_PATH=$NODE_PATH:./lib pm2 stop run.js",
    "pm2_delete": "NODE_PATH=$NODE_PATH:./lib pm2 delete run.js",
    "pm2_restart": "NODE_PATH=$NODE_PATH:./lib pm2 restart run.js"
  },
  "pre-commit": [
    "jshint"
  ],
  "engines": {
    "node": ">= 0.10.36"
  }
}
